{
  "version": "1.0.0-*",
  "description": "Microsoft Azure KeyVault encryption support.",
  "packOptions": {
    "repository": {
      "type": "git",
      "url": "git://github.com/aspnet/dataprotection"
    },
    "tags": [
      "aspnetcore",
      "dataprotection",
      "azure",
      "keyvault"
    ]
  },
  "dependencies": {
    "Microsoft.AspNetCore.DataProtection": "1.1.0-*",
    "Microsoft.IdentityModel.Clients.ActiveDirectory": "3.13.5",
    "Microsoft.Azure.KeyVault": "2.0.2-preview"
  },
  "frameworks": {
    "netstandard1.5": { },
    "net451": {
      "frameworkAssemblies": {
        "System.Threading.Tasks": {
          "type": "build"
        },
        "System.Runtime": {
          "type": "build"
        }
      }
    }
  },
  "buildOptions": {
    "allowUnsafe": true,
    "warningsAsErrors": true,
    "keyFile": "../../tools/Key.snk",
    "nowarn": [
      "CS1591"
    ],
    "xmlDoc": true
  }
}